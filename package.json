{
  "name": "training",
  "version": "1.0.0",
  "description": "training",
  "main": "index.js",
  "private": true,
  "keywords": [
    "training"
  ],
  "author": "RG",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch:all": "jest --watchAll",
    "test:watch:this": "jest --watch",
    "test:these": "jest --findRelatedTests",
    "test:spec": "jest -t",
    "test:collect:coverage:": "jest --collectCoverage",
    "test:update:snapshot": "jest --update-snapshot --detectOpenHandles",
    "lint": "npx eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development --node-env=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tailwindcss": "^2.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@types/jest": "^27.0.2",
    "@webpack-cli/generators": "^2.3.0",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.4",
    "jest-extended": "^1.1.0",
    "mini-css-extract-plugin": "^2.4.1",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.0",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  }
}
